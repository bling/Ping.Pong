										</VisualStateGroup>
									</VisualStateManager.VisualStateGroups>
									<Grid>
										<Rectangle x:Name="Background" Fill="{StaticResource ScreenNameTextBrush}" />
										<Border x:Name="BackgroundMouseOver" Background="White" Opacity="0" />
